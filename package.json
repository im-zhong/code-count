{
  "name": "code-count",
  "displayName": "Code Count",
  "description": "Count all your codes in your workspaces by lines!",
  "icon": "assets/icon.png",
  "version": "0.2.0",
  "publisher": "im-zhong",
  "engines": {
    "vscode": "^1.74.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/im-zhong/code-count"
  },
  "bugs": {
    "url": "https://github.com/im-zhong/code-count/issues"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "icons": {
      "python-icon": {
        "description": "python icon",
        "default": {
          "fontPath": "./assets/fa-brands-400.woff2",
          "fontCharacter": ""
        }
      },
      "cuttlefish-icon": {
        "description": "cuttlefish icon for C/C++/C#",
        "default": {
          "fontPath": "./assets/fa-brands-400.woff2",
          "fontCharacter": ""
        }
      },
      "java-icon": {
        "description": "java icon",
        "default": {
          "fontPath": "./assets/fa-brands-400.woff2",
          "fontCharacter": ""
        }
      },
      "javascript-icon": {
        "description": "javascript icon",
        "default": {
          "fontPath": "./assets/fa-brands-400.woff2",
          "fontCharacter": ""
        }
      },
      "golang-icon": {
        "description": "golang icon",
        "default": {
          "fontPath": "./assets/fa-brands-400.woff2",
          "fontCharacter": ""
        }
      },
      "rust-icon": {
        "description": "rust icon",
        "default": {
          "fontPath": "./assets/fa-brands-400.woff2",
          "fontCharacter": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.11",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-sort-imports": "^1.8.6",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "glob": "^11.0.0",
    "ignore": "^5.3.1"
  }
}
